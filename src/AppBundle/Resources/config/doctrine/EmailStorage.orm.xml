<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\EmailStorage" table="email_storage">
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="email_storage_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="source" type="string" column="source" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idAddress" type="string" column="id_address" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="registrationDate" type="datetime" column="registration_date" nullable="true"/>
    <field name="browser" type="string" column="browser" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="browserVersion" type="string" column="browser_version" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="hash" type="string" column="hash" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
