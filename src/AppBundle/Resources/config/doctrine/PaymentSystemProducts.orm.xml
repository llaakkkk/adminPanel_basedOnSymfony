<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\PaymentSystemProducts" table="payment_system_products">
    <indexes>
      <index name="IDX_DDD8077B2C55C7C8" columns="license_type_id"/>
      <index name="IDX_DDD8077B93BC008D" columns="payment_system_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="payment_system_products_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="productId" type="string" column="product_id" length="32" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <many-to-one field="licenseType" target-entity="UserBundle\Entity\LicenseTypes" fetch="LAZY">
      <join-columns>
        <join-column name="license_type_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="paymentSystem" target-entity="PaymentSystems" fetch="LAZY">
      <join-columns>
        <join-column name="payment_system_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
