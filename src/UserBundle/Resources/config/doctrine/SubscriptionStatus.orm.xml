<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\SubscriptionStatus" table="subscription_status">
    <indexes>
      <index name="IDX_B2F67D9B4584665A" columns="product_id"/>
      <index name="IDX_B2F67D9BD3B6F523" columns="license_status_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="subscription_status_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nextChargeDate" type="datetime" column="next_charge_date" nullable="true"/>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <many-to-one field="product" target-entity="MarketingBundle\Entity\PaymentSystemProducts" fetch="LAZY">
      <join-columns>
        <join-column name="product_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="licenseStatus" target-entity="LicenseStatus" fetch="LAZY">
      <join-columns>
        <join-column name="license_status_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
