<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\UserTracking" table="user_tracking">
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="user_tracking_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="userDeviceId" type="integer" column="user_device_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="hash" type="string" column="hash" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="domain" type="string" column="domain" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="clientId" type="string" column="client_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gClickId" type="string" column="g_click_id" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmSource" type="string" column="utm_source" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmMedium" type="string" column="utm_medium" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmCampaign" type="string" column="utm_campaign" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmAdgroup" type="string" column="utm_adgroup" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmTerm" type="string" column="utm_term" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmMatchtype" type="string" column="utm_matchtype" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmContent" type="string" column="utm_content" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utmDevice" type="string" column="utm_device" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
