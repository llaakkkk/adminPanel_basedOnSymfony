<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserBundle\Entity\Users" table="users" repository-class="UserBundle\Repository\UsersRepository">
    <unique-constraints>
      <unique-constraint name="users_email_uindex" columns="email"/>
    </unique-constraints>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="users_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="firstName" type="string" column="first_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="last_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
    <field name="updated" type="datetime" column="updated" nullable="false">
      <options>
        <option name="default">now()</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
